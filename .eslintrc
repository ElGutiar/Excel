{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
    "semi": "off",
    "no-undef": "off",
    "operator-linebreak": "off",
    "no-unused-vars": "off",
    "object-curly-spacing": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "linebreak-style": "off"
    },
    "extends": ["eslint:recommended", "google"]
} 